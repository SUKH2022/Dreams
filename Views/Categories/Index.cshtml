<h1>Categories</h1>
<table class="table table-striped table-bordered">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
        </tr>
    </thead>

    <tbody>
        @foreach(var category in Model){
            <tr>
                <td>@category["Id"]</td>
                <td>@category["Name"]</td>
            </tr>
        }
    </tbody>
</table>